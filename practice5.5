#include <stdio.h>

void main()
{
      int score1, score2, score3, sum;
      
      printf("3 과목의 성적을 입력하시오(0~100):");
      scanf_s("%d %d %d", &score1, &score2, &score3);
      sum = score1 + score2 + score3;
      if( sum >= 180){
            if(score1 < 40 | score2 < 40 | score3 < 40)
                printf("Subject Failure \n");
            else 
                printf("Success \n");
      }
      else
          printf("Failure \n");
}
